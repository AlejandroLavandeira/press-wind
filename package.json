{
    "name": "press-wind",
    "version": "2.0.0",
    "description": "Wordpress theme starter with Tailwind CSS and Vite JS for FSE",
    "main": "main.js",
    "type": "module",
    "author": "Faramaz Patrick<info@goodmotion.fr>",
    "license": "ISC",
    "scripts": {
        "dev": "cross-env APP_ENV=development vite & npm run watch:editor",
        "build": "APP_ENV=production vite build && npm run build:editor && npm run build:admin",
        "dev:admin": "cross-env APP_ENV=development vite --config admin/vite.config.js",
        "build:admin": "APP_ENV=production vite build --config admin/vite.config.js",
        "watch:editor": "APP_ENV=development IS_EDITOR=true node_modules/.bin/postcss -w --verbose --no-map assets/css/styles.css -o style-editor.css",
        "build:editor": "vite build --config vite.config.editor.js",
        "favicon": "node js-helpers/faviconGenerator.mjs",
        "php:lint": "./vendor/bin/duster lint",
        "php:fix": "./vendor/bin/duster fix"
    },
    "trustedDependencies": [
        "sharp"
    ],
    "devDependencies": {
        "@_tw/themejson": "0.2.1",
        "@biomejs/biome": "^2.2.5",
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/postcss": "^4.1.14",
        "@tailwindcss/vite": "^4.1.14",
        "@vitejs/plugin-basic-ssl": "^2.1.0",
        "@vitejs/plugin-legacy": "7.2.1",
        "@vitejs/plugin-react": "5.0.4",
        "@wordpress/browserslist-config": "6.32.0",
        "@wordpress/components": "30.5.0",
        "@wordpress/compose": "7.32.0",
        "@wordpress/data": "10.32.0",
        "@wordpress/edit-post": "8.32.0",
        "@wordpress/element": "6.32.0",
        "@wordpress/eslint-plugin": "22.18.0",
        "@wordpress/i18n": "6.5.0",
        "@wordpress/icons": "10.32.0",
        "@wordpress/plugins": "7.32.0",
        "@wordpress/scripts": "30.25.0",
        "@wordpress/stylelint-config": "23.24.0",
        "autoprefixer": "^10.4.21",
        "babel-preset-env": "^1.7.0",
        "browserslist": "^4.26.3",
        "classnames": "^2.5.1",
        "cross-env": "^10.1.0",
        "favicons": "^7.2.0",
        "lightningcss": "^1.30.2",
        "lightningcss-cli": "^1.30.2",
        "postcss": "8.5.6",
        "postcss-cli": "11.0.1",
        "postcss-import": "^16.1.1",
        "postcss-preset-env": "^10.4.0",
        "postcss-url": "^10.1.3",
        "sharp": "^0.34.4",
        "tailwindcss": "4.1.14",
        "terser": "5.44.0",
        "vite": "7.1.8",
        "vite-plugin-environment": "^1.1.3",
        "vite-plugin-live-reload": "3.0.5"
    },
    "dependencies": {
        "npm-check-updates": "^19.0.0"
    },
    "browserslist": [
        "extends @wordpress/browserslist-config"
    ]
}
